{
  "name": "composable-styled-components",
  "version": "0.0.11",
  "description": "Composable Styled Components",
  "main": "dist/composable-styled-components.js",
  "module": "dist/composable-styled-components.esm.js",
  "browser": "dist/composable-styled-components.cjs.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup -c",
    "checkbuild": "es-check es5 './dist/composable-styled-components.js' './dist/composable-styled-components.cjs.js'",
    "docs": "next dev -p 8080",
    "docs:build": "next build && next export",
    "docs:stagebuild": "NODE_ENV=production inStaging=true next build",
    "docs:stage": "NODE_ENV=production inStaging=true next build && npm run stage",
    "start": "next start -p 8080",
    "stage": "NODE_ENV=production inStaging=true next start -p 8080",
    "test": "NODE_ENV=testing jest --config ./jest.json",
    "test:cov": "NODE_ENV=testing jest --config ./jest.json --coverage",
    "test:e2e": "NODE_ENV=staging cypress open --project ./e2e",
    "test:e2erun": "npm run docs:stagebuild && concurrently --success first --kill-others \"npm run start\" \"NODE_ENV=production cypress run --config video=false --project ./e2e\"",
    "test:watch": "NODE_ENV=testing jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "NODE_ENV=testing jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattcarlotta/composable-styled-components.git"
  },
  "keywords": [
    "react",
    "component",
    "compose",
    "styled-components"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/composable-styled-components.git/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "address": "^1.1.2",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.14.0",
    "concurrently": "^5.3.0",
    "cypress": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "es-check": "^5.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "jest-resolve": "^26.4.0",
    "jsdom": "^16.4.0",
    "next": "^9.5.2",
    "prettier": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-live": "^2.2.2",
    "react-syntax-highlighter": "^13.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^6.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^4.0.0"
  },
  "homepage": "https://composable.mattcarlotta.sh",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "styled-components": "^5.1.1"
  }
}
