{
  "name": "composable-styled-components",
  "version": "0.0.1",
  "description": "Composable Styled Components",
  "main": "dist/index.js",
  "module": "dist/esm/index.esm.js",
  "browser": "dist/umd/index.umd.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "checkbuild": "es-check es5 './dist/index.js' './dist/umd/index.umd.js' './docs/**/*.js'",
    "docs": "next dev -p 8080",
    "start": "next start -p 8080",
    "test": "cross-env NODE_ENV=testing jest --config ./jest.json",
    "test:cov": "cross-env NODE_ENV=testing jest --config ./jest.json --coverage",
    "test:watch": "cross-env NODE_ENV=testing jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "cross-env NODE_ENV=testing jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattcarlotta/composable-styled-components.git"
  },
  "keywords": [
    "react",
    "component",
    "compose",
    "styled-components"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/composable-styled-components.git/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^13.0.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "address": "^1.1.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.13.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "es-check": "^5.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-resolve": "^26.1.0",
    "jest-styled-components": "^7.0.2",
    "jsdom": "^16.3.0",
    "next": "^9.4.4",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-syntax-highlighter": "^12.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^6.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^4.0.0"
  },
  "homepage": "https://mattcarlotta.github.io/composable-styled-components",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "styled-components": "^5.1.1"
  }
